{
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "rules": {
    "array-type": [true, "generic"],
    "no-submodule-imports": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "prefer-object-spread": false,
    "no-implicit-dependencies": [true, "dev", "optional"]
  }
}
